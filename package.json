{
  "name": "@electron/get",
  "version": "1.0.0",
  "description": "Utility for downloading artifacts from different versions of Electron",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "repository": "https://github.com/electron/get",
  "author": "Samuel Attard",
  "license": "MIT",
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "prepare": "npm run build",
    "test": "jest --coverage"
  },
  "files": [
    "dist/*",
    "README.md"
  ],
  "engines": {
    "node": ">=8.6"
  },
  "dependencies": {
    "env-paths": "^2.1.0",
    "fs-extra": "^8.0.1",
    "got": "^9.6.0",
    "sumchecker": "^3.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^7.0.0",
    "@types/got": "^9.4.4",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.0",
    "typescript": "^3.4.5"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "electron",
    "download",
    "prebuild",
    "get",
    "artifact",
    "release"
  ]
}
